ekstraksi.csv')\n","label_encoder = joblib.load('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/models/label_encoder.pkl')\n","\n","text_column = 'amar_lainnya'\n","label_column = 'klasifikasi'\n","case_solutions = dict(zip(df[label_column], df[text_column]))\n","\n","with open('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/queries.json', 'r') as f:\n","    queries = json.load(f)\n","\n","# === 3. Load Model & Vectorizer ===\n","vectorizer = joblib.load('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/models/vectorizer_tfidf.pkl')\n","nb_model = joblib.load('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/models/naive_bayes_tfidf.pkl')\n","svm_model = joblib.load('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/models/svm_tfidf.pkl')\n","svm_bert_model = joblib.load('/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/models/svm_bert.pkl')\n","\n","tokenizer = AutoTokenizer.from_pretrained(\"indobenchmark/indobert-base-p1\")\n","model_bert = AutoModel.from_pretrained(\"indobenchmark/indobert-base-p1\").to('cuda' if torch.cuda.is_available() else 'cpu')\n","\n","# === 4. Embedding BERT ===\n","def get_bert_embedding(text_list):\n","    embeddings = []\n","    device = next(model_bert.parameters()).device\n","    with torch.no_grad():\n","        for text in text_list:\n","            inputs = tokenizer(text, return_tensors='pt', truncation=True, padding=True, max_length=512)\n","            inputs = {k: v.to(device) for k, v in inputs.items()}\n","            outputs = model_bert(**inputs)\n","            cls_embedding = outputs.last_hidden_state[:, 0, :].squeeze().cpu().numpy()\n","            embeddings.append(cls_embedding)\n","    return np.array(embeddings)\n","\n","# === 5. Retrieval Functions ===\n","def retrieve_tfidf(query, model, k=5):\n","    vec = vectorizer.transform([query])\n","    if hasattr(model, \"predict_proba\"):\n","        scores = model.predict_proba(vec)[0]\n","    else:\n","        scores = model.decision_function(vec)[0]\n","    top_k_idx = scores.argsort()[::-1][:k]\n","    return [label_encoder.inverse_transform([i])[0] for i in top_k_idx]\n","\n","def retrieve_bert(query, model, k=5):\n","    vec = get_bert_embedding([query])\n","    scores = model.decision_function(vec)[0]\n","    top_k_idx = scores.argsort()[::-1][:k]\n","    return [label_encoder.inverse_transform([i])[0] for i in top_k_idx]\n","\n","# === 6. Evaluasi Retrieval Metrics ===\n","def eval_retrieval(queries, model, retrieve_fn, k=5, model_name=''):\n","    y_true = []\n","    y_pred = []\n","\n","    for q in queries:\n","        query_text = q['query']\n","        ground_truth = q['ground_truth']\n","        pred_top_k = retrieve_fn(query_text, model, k)\n","\n","        y_true.append(ground_truth)\n","        y_pred.append(pred_top_k[0] if pred_top_k else 'UNKNOWN')\n","\n","    # Encode untuk sklearn metrics\n","    valid_labels = list(set(y_true) | set(y_pred))\n","    le = LabelEncoder().fit(valid_labels)\n","    y_true_enc = le.transform(y_true)\n","    y_pred_enc = le.transform(y_pred)\n","\n","    metrics = {\n","        \"Model\": model_name,\n","        \"Accuracy\": accuracy_score(y_true_enc, y_pred_enc),\n","        \"Precision\": precision_score(y_true_enc, y_pred_enc, average='macro', zero_division=0),\n","        \"Recall\": recall_score(y_true_enc, y_pred_enc, average='macro', zero_division=0),\n","        \"F1-Score\": f1_score(y_true_enc, y_pred_enc, average='macro', zero_division=0)\n","    }\n","    return metrics\n","\n","# === 7. Jalankan Evaluasi Semua Model ===\n","results = []\n","results.append(eval_retrieval(queries, nb_model, retrieve_tfidf, k=5, model_name='TF-IDF + Naive Bayes'))\n","results.append(eval_retrieval(queries, svm_model, retrieve_tfidf, k=5, model_name='TF-IDF + SVM'))\n","results.append(eval_retrieval(queries, svm_bert_model, retrieve_bert, k=5, model_name='BERT + SVM'))\n","\n","# === 8. Simpan ke CSV ===\n","eval_dir = '/content/drive/MyDrive/Tugas/Penalaran Komputer/Tugas UAS/eval'\n","os.makedirs(eval_dir, exist_ok=True)\n","\n","df_metrics = pd.DataFrame(results)\n","df_metrics.to_csv(f'{eval_dir}/retrieval_metrics.csv', index=False)\n","print(f\"\\nâœ… Retrieval metrics disimpan di: {eval_dir}/retrieval_metrics.csv\")\n","\n","# === 9. Visualisasi Opsional ===\n","sns.barplot(data=df_metrics.melt(id_vars='Model'), x='variable', y='value', hue='Model')\n","plt.title('Perbandingan Performa Model')\n","plt.ylabel('Score')\n","plt.show()\n"]}]}